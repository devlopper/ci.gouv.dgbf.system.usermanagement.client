<?xml version="1.0" encoding="UTF-8"?>
<faces-config version="2.2" xmlns="http://xmlns.jcp.org/xml/ns/javaee"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd">

	<!-- Do use PrettyFaces ONLY for navigation -->

	<navigation-rule>
		<from-view-id>*</from-view-id>
		<!--
		<navigation-case>
			<from-outcome>scopeTypeListView</from-outcome>
			<to-view-id>/private/scope/type/list.jsf</to-view-id>
			<redirect />
		</navigation-case>
		
		<navigation-case>
			<from-outcome>scopeTypeEditView</from-outcome>
			<to-view-id>/private/scope/type/edit.jsf</to-view-id>
			<redirect />
		</navigation-case>
		-->
		<navigation-case>
			<from-outcome>accountEditMyPassView</from-outcome>
			<to-view-id>/private/account/editmypass.jsf</to-view-id>
			<redirect />
		</navigation-case>
		
		<navigation-case>
			<from-outcome>accountScopeListView</from-outcome>
			<to-view-id>/private/useraccount/scope/list.jsf</to-view-id>
			<redirect />
		</navigation-case>
		
		<navigation-case>
			<from-outcome>userAccountListView</from-outcome>
			<to-view-id>/private/useraccount/list.jsf</to-view-id>
			<redirect />
		</navigation-case>
		
		<navigation-case>
			<from-outcome>userAccountCreateView</from-outcome>
			<to-view-id>/private/useraccount/create.jsf</to-view-id>
			<redirect />
		</navigation-case>
		
		<navigation-case>
			<from-outcome>userAccountReadView</from-outcome>
			<to-view-id>/private/useraccount/read.jsf</to-view-id>
			<redirect />
		</navigation-case>
		
		<navigation-case>
			<from-outcome>userAccountEditMyProfile</from-outcome>
			<to-view-id>/private/useraccount/editmyprofile.jsf</to-view-id>
			<redirect />
		</navigation-case>
		
		<navigation-case>
			<from-outcome>userAccountEditMyNotation</from-outcome>
			<to-view-id>/private/useraccount/editmynotation.jsf</to-view-id>
			<redirect />
		</navigation-case>
		
		<navigation-case>
			<from-outcome>userAccountEditMyPreferences</from-outcome>
			<to-view-id>/private/useraccount/editmypreferences.jsf</to-view-id>
			<redirect />
		</navigation-case>
		
		<navigation-case>
			<from-outcome>userAccountCreateListUserView</from-outcome>
			<to-view-id>/private/useraccount/listuser.jsf</to-view-id>
			<redirect />
		</navigation-case>
			
		<navigation-case>
			<from-outcome>profileListView</from-outcome>
			<to-view-id>/private/profile/list.jsf</to-view-id>
			<redirect />
		</navigation-case>
		
		<navigation-case>
			<from-outcome>userAccountAssignScopesListUserAccountView</from-outcome>
			<to-view-id>/private/useraccount/scope/assign/listuseraccount.jsf</to-view-id>
			<redirect />
		</navigation-case>
				
		<navigation-case>
			<from-outcome>userAccountAssignScopesProcessUserAccountView</from-outcome>
			<to-view-id>/private/useraccount/scope/assign/processuseraccount.jsf</to-view-id>
			<redirect />
		</navigation-case>
		
		<navigation-case>
			<from-outcome>userAccountAssignPrivilegesListUserAccountView</from-outcome>
			<to-view-id>/private/useraccount/privilege/assign/listuseraccount.jsf</to-view-id>
			<redirect />
		</navigation-case>
		
		<navigation-case>
			<from-outcome>userAccountAssignPrivilegesProcessUserAccountView</from-outcome>
			<to-view-id>/private/useraccount/privilege/assign/processuseraccount.jsf</to-view-id>
			<redirect />
		</navigation-case>
		
	</navigation-rule>

</faces-config>
